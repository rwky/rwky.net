*
  font-family "Courier New",Courier,sans-serif
  color #0F0
  
html
  background #000
  font-size 16px

input, button, textarea
  background #0F0
  color #000
  padding 5px
  font-weight bold
  border 1px solid

.alert
  font-weight bold
  color #000
  background #0F0
  border 1px solid
  margin 15px 0
  padding 5px

footer
  text-align center
  margin-top 20px
  border-top 1px solid
  padding-bottom 10px
  padding-top 10px

