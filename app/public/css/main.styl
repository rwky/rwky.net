*
  font-family:"Helvetica Neue","Helvetica","Arial",sans-serif
  
img
  max-width:100%
  max-height:100%
  
h4
  font-weight:100
  font-size:22px
ul
  margin:0
  padding:0
  list-style:disc inside none

.clear
  clear:both
  float:none

p
  margin:10px 0
  
.navbar-inverse
  background-image:none
  background-color:#16638D
  border-radius:0
  border-color:#16638D
  color:#fff
  .navbar-nav
    > li
      > a
        color:#ddd
    li.active > a, li.active > a:hover
      background-color:#114268
  
.navbar-inverse .navbar-brand
  color:#ddd

body.server-administration,body.programming
  h4
    text-align:center
    
body.testimonials .panel-body, body.programming p, li
  line-height:170%

footer
  text-align:center
  margin-top:20px
  border-top:1px solid #E5E5E5
  padding-bottom:10px
  padding-top:10px

